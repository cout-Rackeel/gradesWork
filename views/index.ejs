<h2 class="mt-3 mx-2 mb-3 h3" >
  Welcome to GradesWeb - This is our Letter grading system
</h2>

<table class="mt-5 mx-auto table " style="width:100%; text-align: center;">
  <thead>
    <tr>
      <th>Letter Grade</th>
      <th>Description</th>
      <th>Result</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach(letter => { %>
      <tr>
      <td><%=letter.letter %></td>
      <td><%=letter.description %></td>
      <td><%=letter.meaning %></td>
      </tr>
      
  <% }) %>
  </tbody>

</table>